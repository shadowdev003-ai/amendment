  Button = {Image = "rbxassetid://83554422321074", BackgroundTransparency = 0},
  Corner = {CornerRadius = UDim.new(35, 1)}
})

task.defer(function()
  Window:Dialog({
      Title = "مرحبا بك", 
      Text = "اتمنى ان يعجبك السكربت ",
      Options = {
          {"قفل", function()  
              if MainFrame:FindFirstChild("Dialog") then
                  MainFrame.Dialog:Destroy()
              end
          end},
          {"لاحقا", function()
              if MainFrame:FindFirstChild("Dialog") then
                  MainFrame.Dialog:Destroy()
              end
          end}
      }
  })
end)

