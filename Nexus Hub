CameraTab = Window:MakeTab({"الكاميرا"})

CameraTab:AddSection({"في اشياء ممكن تعليق معك بس بالحقيقه هو شغال"})

local cameraSettings = {
    fov = 70,
    distance = 10,
    height = 2,
    smoothness = 0.1,
    lockOn = false,
    freeCam = false,
    thirdPerson = false,
    noclipCam = false
}

CameraTab:AddSlider({
    Name = "مجال الرؤية",
    Min = 30,
    Max = 120,
    Default = 70,
    Color = Color3.fromRGB(128, 0, 128),
    Increment = 1,
    ValueName = "درجة",
    Callback = function(value)
        cameraSettings.fov = value
        game.Workspace.CurrentCamera.FieldOfView = value
    end
})

CameraTab:AddButton({
    Name = "إعادة مجال الرؤية للطبيعي",
    Callback = function()
        cameraSettings.fov = 70
        game.Workspace.CurrentCamera.FieldOfView = 70
    end
})

CameraTab:AddSection({"أوضاع الكاميرا"})

CameraTab:AddToggle({
    Name = "كاميرا الشخص الثالث",
    Default = false,
    Callback = function(value)
        cameraSettings.thirdPerson = value
        local player = game.Players.LocalPlayer
        local character = player.Character
        
        if value and character then
            local humanoid = character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.CameraOffset = Vector3.new(0, 0, -cameraSettings.distance)
            end
        else
            if character then
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.CameraOffset = Vector3.new(0, 0, 0)
                end
            end
        end
    end
})

CameraTab:AddSlider({
    Name = "مسافة الكاميرا",
    Min = 2,
    Max = 50,
    Default = 10,
    Color = Color3.fromRGB(128, 0, 128),
    Increment = 0.5,
    ValueName = "خطوة",
    Callback = function(value)
        cameraSettings.distance = value
        if cameraSettings.thirdPerson then
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character then
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.CameraOffset = Vector3.new(0, cameraSettings.height, -value)
                end
            end
        end
    end
})

CameraTab:AddSlider({
    Name = "ارتفاع الكاميرا",
    Min = -10,
    Max = 10,
    Default = 2,
    Color = Color3.fromRGB(128, 0, 128),
    Increment = 0.5,
    ValueName = "خطوة",
    Callback = function(value)
        cameraSettings.height = value
        if cameraSettings.thirdPerson then
            local player = game.Players.LocalPlayer
            local character = player.Character
            if character then
                local humanoid = character:FindFirstChild("Humanoid")
                if humanoid then
                    humanoid.CameraOffset = Vector3.new(0, value, -cameraSettings.distance)
                end
            end
        end
    end
})

local freeCamConnection = nil

CameraTab:AddToggle({
    Name = "كاميرا حرة تجريبيه",
    Default = false,
    Callback = function(value)
        cameraSettings.freeCam = value
        local camera = workspace.CurrentCamera
        local player = game.Players.LocalPlayer
        
        if value then
            local character = player.Character
            if character and character:FindFirstChild("HumanoidRootPart") then
                local camPos = character.HumanoidRootPart.Position + Vector3.new(0, 5, 10)
                camera.CameraType = Enum.CameraType.Scriptable
                camera.CFrame = CFrame.new(camPos, character.HumanoidRootPart.Position)
                
                local speed = 1
                local keys = {}
                
                freeCamConnection = game:GetService("RunService").RenderStepped:Connect(function()
                    local moveVec = Vector3.new(0, 0, 0)
                    if keys[Enum.KeyCode.W] then moveVec = moveVec + camera.CFrame.LookVector end
                    if keys[Enum.KeyCode.S] then moveVec = moveVec - camera.CFrame.LookVector end
                    if keys[Enum.KeyCode.A] then moveVec = moveVec - camera.CFrame.RightVector end
                    if keys[Enum.KeyCode.D] then moveVec = moveVec + camera.CFrame.RightVector end
                    if keys[Enum.KeyCode.E] then moveVec = moveVec + Vector3.new(0, 1, 0) end
                    if keys[Enum.KeyCode.Q] then moveVec = moveVec - Vector3.new(0, 1, 0) end
                    
                    camera.CFrame = camera.CFrame + moveVec * speed
                end)
                
                game:GetService("UserInputService").InputBegan:Connect(function(input)
                    keys[input.KeyCode] = true
                end)
                
                game:GetService("UserInputService").InputEnded:Connect(function(input)
                    keys[input.KeyCode] = false
                end)
            end
        else
            if freeCamConnection then
                freeCamConnection:Disconnect()
                freeCamConnection = nil
            end
            camera.CameraType = Enum.CameraType.Custom
            local character = player.Character
            if character and character:FindFirstChild("Humanoid") then
                camera.CameraSubject = character.Humanoid
            end
        end
    end
})

CameraTab:AddSlider({
    Name = "سرعة الكاميرا الحرة",
    Min = 0.1,
    Max = 10,
    Default = 1,
    Color = Color3.fromRGB(128, 0, 128),
    Increment = 0.1,
    ValueName = "ضعف",
    Callback = function(value)
    end
})

CameraTab:AddSection({"ببعض الاحيان الكاميرا ممكن تعلق ضفتلكم ذا"})

CameraTab:AddToggle({
    Name = "مضاد قفل الزوم",
    Default = false,
    Callback = function(value)
        if value then
            game:GetService("RunService").Heartbeat:Connect(function()
                local camera = workspace.CurrentCamera
                if camera then
                    if camera.CameraType == Enum.CameraType.Scriptable then
                        camera.CameraType = Enum.CameraType.Custom
                    end
                end
            end)
        end
    end
})

local antiCameraLockConnection = nil

CameraTab:AddToggle({
    Name = "مضاد تجميد الكاميرا",
    Default = false,
    Callback = function(value)
        if value then
            antiCameraLockConnection = game:GetService("RunService").Heartbeat:Connect(function()
                local player = game.Players.LocalPlayer
                local character = player.Character
                if character then
                    local humanoid = character:FindFirstChild("Humanoid")
                    if humanoid and workspace.CurrentCamera.CameraSubject ~= humanoid then
                        workspace.CurrentCamera.CameraSubject = humanoid
                    end
                end
            end)
        else
            if antiCameraLockConnection then
                antiCameraLockConnection:Disconnect()
            end
        end
    end
})

local antiShakeConnection = nil

CameraTab:AddToggle({
    Name = "مضاد اهتزار كاميرا",
    Default = false,
    Callback = function(value)
        if value then
            local lastCFrame = workspace.CurrentCamera.CFrame
            antiShakeConnection = game:GetService("RunService").RenderStepped:Connect(function()
                local camera = workspace.CurrentCamera
                if camera then
                    local currentCFrame = camera.CFrame
                    local diff = (currentCFrame.Position - lastCFrame.Position).Magnitude
                    if diff > 10 then
                        camera.CFrame = lastCFrame
                    else
                        lastCFrame = currentCFrame
                    end
                end
            end)
        else
            if antiShakeConnection then
                antiShakeConnection:Disconnect()
            end
        end
    end
})

CameraTab:AddSection({"تثبيت كامرتك علي الاعب الي تختاره"})

local lockOnConnection = nil
local selectedTarget = nil

local targetDropdown = CameraTab:AddDropdown({
    Name = "اختر لاعب للتثبيت",
    Options = {},
    Default = "",
    Callback = function(value)
        selectedTarget = value
    end
})

local function updateTargetList()
    local players = {}
    for _, p in ipairs(game.Players:GetPlayers()) do
        if p ~= game.Players.LocalPlayer then
            table.insert(players, p.Name)
        end
    end
    targetDropdown:Refresh(players, true)
end

CameraTab:AddButton({
    Name = "تحديث قائمة اللاعبين",
    Callback = function()
        updateTargetList()
    end
})

game.Players.PlayerAdded:Connect(updateTargetList)
game.Players.PlayerRemoving:Connect(updateTargetList)

CameraTab:AddToggle({
    Name = "تثبيت الكاميرا على اللاعب",
    Default = false,
    Callback = function(value)
        cameraSettings.lockOn = value
        if value then
            if not selectedTarget then
                return
            end
            lockOnConnection = game:GetService("RunService").Heartbeat:Connect(function()
                local target = game.Players:FindFirstChild(selectedTarget)
                local player = game.Players.LocalPlayer
                if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                    local camera = workspace.CurrentCamera
                    local targetPos = target.Character.HumanoidRootPart.Position
                    local playerChar = player.Character
                    if playerChar and playerChar:FindFirstChild("HumanoidRootPart") then
                        local playerPos = playerChar.HumanoidRootPart.Position
                        camera.CFrame = CFrame.new(playerPos, targetPos)
                    end
                end
            end)
        else
            if lockOnConnection then
                lockOnConnection:Disconnect()
                lockOnConnection = nil
            end
        end
    end
})

CameraTab:AddSection({"المميزات الإضافية"})

CameraTab:AddToggle({
    Name = "إظهار الجسم في الشخص الأول",
    Default = false,
    Callback = function(value)
        local player = game.Players.LocalPlayer
        if value then
            player.DevCameraOcclusionMode = Enum.DevCameraOcclusionMode.Invisicam
        else
            player.DevCameraOcclusionMode = Enum.DevCameraOcclusionMode.Zoom
        end
    end
})

CameraTab:AddToggle({
    Name = "إخفاء الواجهة للصور",
    Default = false,
    Callback = function(value)
        local player = game.Players.LocalPlayer
        local gui = player:FindFirstChild("PlayerGui")
        if gui then
            for _, g in ipairs(gui:GetChildren()) do
                if g:IsA("ScreenGui") then
                    g.Enabled = not value
                end
            end
        end
        game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.All, not value)
    end
})

CameraTab:AddSection({"تأثيرات الكاميرا"})

CameraTab:AddToggle({
    Name = "طمس الحركة",
    Default = false,
    Callback = function(value)
        local lighting = game:GetService("Lighting")
        local blur = lighting:FindFirstChild("CameraMotionBlur")
        if value then
            if not blur then
                blur = Instance.new("BlurEffect")
                blur.Name = "CameraMotionBlur"
                blur.Size = 10
                blur.Parent = lighting
            end
        else
            if blur then
                blur:Destroy()
            end
        end
    end
})

CameraTab:AddToggle({
    Name = "تكبير تلقائي على الاشخاص القريبة",
    Default = false,
    Callback = function(value)
        if value then
            game:GetService("RunService").Heartbeat:Connect(function()
                local player = game.Players.LocalPlayer
                local character = player.Character
                if not character then return end
                
                local closestPlayer = nil
                local closestDistance = 999999
                
                for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
                    if otherPlayer ~= player and otherPlayer.Character then
                        local otherChar = otherPlayer.Character
                        local otherHRP = otherChar:FindFirstChild("HumanoidRootPart")
                        local myHRP = character:FindFirstChild("HumanoidRootPart")
                        
                        if otherHRP and myHRP then
                            local distance = (otherHRP.Position - myHRP.Position).Magnitude
                            if distance < closestDistance and distance < 20 then
                                closestDistance = distance
                                closestPlayer = otherPlayer
                            end
                        end
                    end
                end
                
                if closestPlayer then
                    workspace.CurrentCamera.FieldOfView = 30 + (closestDistance / 20) * 40
                else
                    workspace.CurrentCamera.FieldOfView = cameraSettings.fov
                end
            end)
        else
            workspace.CurrentCamera.FieldOfView = cameraSettings.fov
        end
    end
})

CameraTab:AddSection({"معلومات الكاميرا"})

local camInfoLabel = CameraTab:AddParagraph({
    Title = "موقع الكاميرا",
    Text = "س: 0, ص: 0, ع: 0"
})

local camRotLabel = CameraTab:AddParagraph({
    Title = "دوران الكاميرا",
    Text = "س: 0°, ص: 0°, ع: 0°"
})

game:GetService("RunService").Heartbeat:Connect(function()
    local camera = workspace.CurrentCamera
    if camera then
        local pos = camera.CFrame.Position
        local rot = camera.CFrame.Rotation:ToEulerAnglesXYZ()
        camInfoLabel:SetText(string.format("س: %.2f, ص: %.2f, ع: %.2f", pos.X, pos.Y, pos.Z))
        camRotLabel:SetText(string.format("س: %.0f°, ص: %.0f°, ع: %.0f°", 
            math.deg(rot.X), math.deg(rot.Y), math.deg(rot.Z)))
    end
end)

CameraTab:AddButton({
    Name = "إعادة ضبط الكاميرا",
    Callback = function()
        local camera = workspace.CurrentCamera
        local player = game.Players.LocalPlayer
        camera.FieldOfView = 70
        camera.CameraType = Enum.CameraType.Custom
        local character = player.Character
        if character and character:FindFirstChild("Humanoid") then
            camera.CameraSubject = character.Humanoid
        end
        cameraSettings.thirdPerson = false
        cameraSettings.freeCam = false
        cameraSettings.lockOn = false
        if lockOnConnection then
            lockOnConnection:Disconnect()
        end
        if freeCamConnection then
            freeCamConnection:Disconnect()
        end
    end
})

ChatTab = Window:MakeTab({"الشات"})

ChatTab:AddSection({"تحكم ب شات الاعبين"})

local chasingplayer = nil
local controlConnection = nil
local originalSpeed = 16
local originalCFrame = nil
local isControlling = false

local function getPlayerList()
    local playerNames = {}
    for _, p in ipairs(game.Players:GetPlayers()) do
        if p ~= game.Players.LocalPlayer then
            table.insert(playerNames, p.Name)
        end
    end
    return playerNames
end

local playerChild = ChatTab:AddDropdown({
    Name = "اختر لاعب",
    Options = getPlayerList(),
    Default = "",
    Callback = function(selected)
        if game.Players:FindFirstChild(selected) then
            chasingplayer = selected
        else
            chasingplayer = nil
        end
    end
})

ChatTab:AddButton({
    Name = "تحديث اسماء الاعبين",
    Callback = function()
        playerChild:Refresh(getPlayerList(), true)
    end
})

game.Players.PlayerAdded:Connect(function()
    task.wait(0.5)
    playerChild:Refresh(getPlayerList(), true)
end)

game.Players.PlayerRemoving:Connect(function()
    task.wait(0.1)
    playerChild:Refresh(getPlayerList(), true)
end)

ChatTab:AddSection({"تأكد ان انت و هو عندكم شات"})

ChatTab:AddToggle({
    Name = "تحكم ب شات الاعب",
    Default = false,
    Callback = function(value)
        if not chasingplayer then
            return
        end
        
        local target = game.Players:FindFirstChild(chasingplayer)
        if not target or not target.Character then
            return
        end
        
        local myChar = game.Players.LocalPlayer.Character
        if not myChar then return end
        
        local myHumanoid = myChar:FindFirstChild("Humanoid")
        if not myHumanoid then return end
        
        if value then
            isControlling = true
            originalSpeed = myHumanoid.WalkSpeed
            local myHRP = myChar:FindFirstChild("HumanoidRootPart")
            if myHRP then
                originalCFrame = myHRP.CFrame
            end
            
            for i = 1, 10 do
                local args = {
                    false
                }
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("IncrementBodySize"):FireServer(unpack(args))
            end
            
            myHumanoid.WalkSpeed = 0
            myHumanoid.JumpPower = 0
            
            local targetHead = target.Character:FindFirstChild("Head")
            
            if myHRP and targetHead then
                myHRP.CFrame = targetHead.CFrame * CFrame.new(0, 0.5, 0)
            end
            
            local chatService = game:GetService("TextChatService")
            if chatService.ChatVersion == Enum.ChatVersion.TextChatService then
                local channel = chatService.TextChannels:FindFirstChild("RBXGeneral")
                if channel then
                    channel:SendAsync("تم تهكير شاتك من قبل NEXUS HUB")
                end
            else
                print("تم التفعيل - TextChatService غير مفعل")
            end
            
            controlConnection = game:GetService("RunService").Heartbeat:Connect(function()
                if not isControlling or not chasingplayer then return end
                local currentTarget = game.Players:FindFirstChild(chasingplayer)
                if currentTarget and currentTarget.Character then
                    local currentHead = currentTarget.Character:FindFirstChild("Head")
                    local currentHRP = myChar:FindFirstChild("HumanoidRootPart")
                    if currentHead and currentHRP then
                        currentHRP.CFrame = currentHead.CFrame * CFrame.new(0, 0.5, 0)
                    end
                end
            end)
        else
            isControlling = false
            if controlConnection then
                controlConnection:Disconnect()
                controlConnection = nil
            end
            
            local myHRP = myChar:FindFirstChild("HumanoidRootPart")
            
            if originalCFrame and myHRP then
                myHRP.CFrame = originalCFrame
            end
            
            for i = 1, 10 do
                local args = {
                    true
                }
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("IncrementBodySize"):FireServer(unpack(args))
            end
            
            if myHumanoid then
                myHumanoid.WalkSpeed = originalSpeed
                myHumanoid.JumpPower = 50
            end
        end
    end
})

Tab = Window:MakeTab({"نسخ سكنات", "rbxassetid://"})
Tab:AddSection({ Name = "نسخ سكنات عدلتهولكم خليته سريع و قوي و ينسخ كل شي" })

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Remotes = ReplicatedStorage:WaitForChild("Remotes")

local Target = nil

local function GetPlayerNames()
    local PlayerNames = {}
    for _, player in ipairs(Players:GetPlayers()) do
        table.insert(PlayerNames, player.Name)
    end
    return PlayerNames
end

local Dropdown = Tab:AddDropdown({
    Name = "اختر الاعب",
    Options = GetPlayerNames(),
    Default = Target,
    Callback = function(Value)
        Target = Value
    end
})

local function UpdateDropdown()
    Dropdown:Refresh(GetPlayerNames(), true)
end

Players.PlayerAdded:Connect(UpdateDropdown)
Players.PlayerRemoving:Connect(UpdateDropdown)

local function FastWear(id)
    if not tonumber(id) then return end
    for i = 1, 5 do
        task.spawn(function()
            pcall(function()
                Remotes.Wear:InvokeServer(tonumber(id))
            end)
        end)
    end
end

local function FastFire(remoteName, ...)
    for i = 1, 5 do
        task.spawn(function()
            pcall(function()
                if Remotes[remoteName] then
                    Remotes[remoteName]:InvokeServer(...)
                end
            end)
        end)
    end
end

local function FastFireServer(remoteName, ...)
    for i = 1, 5 do
        task.spawn(function()
            pcall(function()
                if Remotes[remoteName] then
                    Remotes[remoteName]:FireServer(...)
                end
            end)
        end)
    end
end

Tab:AddButton({
    Name = "نسخ السكن",
    Callback = function()
        if not Target then return end
        local LP = Players.LocalPlayer
        local TPlayer = Players:FindFirstChild(Target)
        if not TPlayer or not TPlayer.Character then return end
        
        local THumanoid = TPlayer.Character:FindFirstChildOfClass("Humanoid")
        if not THumanoid then return end
        
        local PDesc = THumanoid:GetAppliedDescription()
        local argsBody = {[1] = {[1] = PDesc.Torso, [2] = PDesc.RightArm, [3] = PDesc.LeftArm, [4] = PDesc.RightLeg, [5] = PDesc.LeftLeg, [6] = PDesc.Head}}
        
        FastFire("ChangeCharacterBody", unpack(argsBody))
        
        local SkinColor = TPlayer.Character:FindFirstChild("Body Colors")
        if SkinColor then
            FastFireServer("ChangeBodyColor", tostring(SkinColor.HeadColor))
        end
        
        local itemsToWear = {}
        if tonumber(PDesc.Shirt) then table.insert(itemsToWear, PDesc.Shirt) end
        if tonumber(PDesc.Pants) then table.insert(itemsToWear, PDesc.Pants) end
        if tonumber(PDesc.Face) then table.insert(itemsToWear, PDesc.Face) end
        if tonumber(PDesc.IdleAnimation) then table.insert(itemsToWear, PDesc.IdleAnimation) end
        if tonumber(PDesc.RunAnimation) then table.insert(itemsToWear, PDesc.RunAnimation) end
        if tonumber(PDesc.WalkAnimation) then table.insert(itemsToWear, PDesc.WalkAnimation) end
        if tonumber(PDesc.JumpAnimation) then table.insert(itemsToWear, PDesc.JumpAnimation) end
        if tonumber(PDesc.ClimbAnimation) then table.insert(itemsToWear, PDesc.ClimbAnimation) end
        if tonumber(PDesc.FallAnimation) then table.insert(itemsToWear, PDesc.FallAnimation) end
        if tonumber(PDesc.SwimAnimation) then table.insert(itemsToWear, PDesc.SwimAnimation) end
        if tonumber(PDesc.MoodAnimation) then table.insert(itemsToWear, PDesc.MoodAnimation) end
        
        for _, id in ipairs(itemsToWear) do
            FastWear(id)
        end
        
        local accessories = PDesc:GetAccessories(true)
        for _, v in ipairs(accessories) do
            if v.AssetId and tonumber(v.AssetId) then
                FastWear(v.AssetId)
            end
        end
        
        local Bag = TPlayer:FindFirstChild("PlayersBag")
        if Bag then
            if Bag:FindFirstChild("RPName") and Bag.RPName.Value ~= "" then
                FastFireServer("RPNameText", "RolePlayName", Bag.RPName.Value)
            end
            if Bag:FindFirstChild("RPBio") and Bag.RPBio.Value ~= "" then
                FastFireServer("RPNameText", "RolePlayBio", Bag.RPBio.Value)
            end
            if Bag:FindFirstChild("RPNameColor") then
                FastFireServer("RPNameColor", "PickingRPNameColor", Bag.RPNameColor.Value)
            end
            if Bag:FindFirstChild("RPBioColor") then
                FastFireServer("RPNameColor", "PickingRPBioColor", Bag.RPBioColor.Value)
            end
        end
        
        task.spawn(function()
            for i = 1, 3 do
                task.wait(0.3)
                local currentChar = LP.Character
                if not currentChar then continue end
                local currentHum = currentChar:FindFirstChildOfClass("Humanoid")
                if not currentHum then continue end
                local currentDesc = currentHum:GetAppliedDescription()
                if not currentDesc then continue end
                
                if tonumber(PDesc.Shirt) and tostring(currentDesc.Shirt) ~= tostring(PDesc.Shirt) then FastWear(PDesc.Shirt) end
                if tonumber(PDesc.Pants) and tostring(currentDesc.Pants) ~= tostring(PDesc.Pants) then FastWear(PDesc.Pants) end
                if tonumber(PDesc.Face) and tostring(currentDesc.Face) ~= tostring(PDesc.Face) then FastWear(PDesc.Face) end
                
                for _, v in ipairs(accessories) do
                    if v.AssetId and tonumber(v.AssetId) then
                        local found = false
                        for _, currentAcc in ipairs(currentDesc:GetAccessories(true)) do
                            if tostring(currentAcc.AssetId) == tostring(v.AssetId) then
                                found = true
                                break
                            end
                        end
                        if not found then
                            FastWear(v.AssetId)
                        end
                    end
                end
            end
        end)
    end
})
